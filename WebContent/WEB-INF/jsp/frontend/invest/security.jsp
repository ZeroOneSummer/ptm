    <script>
     (function(){
         var lis = document.querySelectorAll('nav>ul>li');
         var sections = document.querySelectorAll('section');
         var heads = document.querySelectorAll('.head');
         var url = getDomain('http://deposit.koudailc.com/page/platform-total');
         $.ajax({
            url: url,
            context: document.body
          }).done(function(data) {
              $('#invest_total').html(data.data.invest_total);
              $('#loan_person').html(data.data.loan_person);
              $('#server_money').html(data.data.server_money);
              $('#profits_total').html(data.data.profits_total);
              $('#user_sum').html(data.data.user_sum);
          });
         [].forEach.call(lis, function(li) {
             li.addEventListener('click', function(e) {
                 for (var i = 0; i < lis.length; i++) {
                     lis[i].className = '';
                     if (lis[i] == e.target) {
                         sections[i].style.display = 'block';
                     } else {
                         sections[i].style.display = 'none';
                     }
                 }
                 e.target.className = 'active';
             })
         });

         [].forEach.call(heads, function(head) {
             head.addEventListener('click', function(e) {
                 var i = this.querySelector('i')
                 if (i.className == '') {
                     i.className = 'down';
                     this.nextElementSibling.style.display = 'none'
                 } else {
                     i.className = '';
                     this.nextElementSibling.style.display = 'block';
                 }
                 document.body.scrollTop+=e.clientY-this.clientHeight/2;
             }, false)
         });

         function getQuery(variable) {
             var query = window.location.search.substring(1);
             var vars = query.split('&');
             for (var i = 0; i < vars.length; i++) {
                 var pair = vars[i].split('=');
                 if (decodeURIComponent(pair[0]) == variable) {
                     return decodeURIComponent(pair[1]);
                 }
             }
             return null
         }
         var tab = getQuery('tab');
         if(tab > 0 && tab < 4){
             if (typeof lis[tab].click === 'function'){
                 lis[tab].click();
             }else{
                 for (var i = 0; i < lis.length; i++) {
                   lis[i].className = '';
                   sections[i].style.display = 'none';
                 }
                 sections[tab].style.display = 'block';
                 lis[tab].className = 'active';
             }
         }}())
    </script>

  

